<view class="content"><block wx:if="{{scriptList.length==0&&script_dir_file.length==0&&onlineScript.length==0}}"><view style="top:80rpx;color:#666666;font-size:28rpx;text-align:center;width:100%;">空空如也</view></block><block wx:for="{{onlineScript}}" wx:for-item="item" wx:for-index="index"><block wx:if="{{item.installPath}}"><view class="list" data-index="{{index}}" data-event-opts="{{[['tap',[['changeScriptOnline',['$event']]]]]}}" bindtap="__e"><view class="left-info"><view class="label">{{item.name}}</view></view><view class="arr"><icon type="{{item.active?'success':'cancel'}}" size="20"></icon></view></view></block></block><block wx:if="{{scriptList.length>0}}"><view style="padding:30rpx;background-color:#F7F7F7;margin-top:30rpx;border-bottom:1px solid #CCCC77;">自定义脚本</view></block><block wx:for="{{scriptList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list" data-index="{{index}}" data-event-opts="{{[['tap',[['showmodal',['$event']]]]]}}" bindtap="__e"><view class="left-info"><view class="label">{{item.name}}</view></view><view class="arr"><icon type="{{item.active?'success':'cancel'}}" size="20"></icon></view></view></block><block wx:if="{{script_dir_file.length>0}}"><view style="padding:30rpx;background-color:#F7F7F7;margin-top:30rpx;border-bottom:1px solid #CCCC77;">本地脚本</view></block><block wx:for="{{script_dir_file}}" wx:for-item="s_self" wx:for-index="index_2"><view class="list" data-index="{{index_2}}" data-event-opts="{{[['tap',[['changeScriptFile',['$event']]]]]}}" bindtap="__e"><view class="left-info"><view class="label">{{s_self.filename}}</view></view><view class="arr"><icon type="{{s_self.active?'success':'cancel'}}" size="20"></icon></view></view></block></view>